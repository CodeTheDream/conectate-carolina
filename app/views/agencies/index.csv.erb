<%- headers = @headers %>
<%=CSV.generate_line headers %><%- Agency.all.each do |agency| -%><%=sanitize raw(CSV.generate_line(
                                    [agency.name,
                                     agency.address,
                                     agency.city,
                                     agency.state,
                                     agency.zipcode,
                                     agency.contact,
                                     agency.phone,
                                     agency.description,
                                     agency.email,
                                     agency.descripcion,
                                     agency.mobile_phone,
                                     agency.categories.first&.name,
                                     agency.categories.first&.categoria,
                                     agency.categories.first&.fa_name,
                                     agency.websites.find_by(website_type: WebsiteType.find_by(name: "Website"))&.url,
                                     agency.websites.find_by(website_type: WebsiteType.find_by(name: "Facebook"))&.url])) -%>
<%- end -%>
