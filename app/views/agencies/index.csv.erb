<%- headers = @headers %>
<%=CSV.generate_line headers %><%- Agency.all.includes(:websites, :categories).each do |agency| -%><%=sanitize raw(CSV.generate_line(
                                    [agency.name,
                                     agency.address,
                                     agency.city,
                                     agency.state,
                                     agency.zipcode,
                                     agency.contact,
                                     agency.phone,
                                     agency.description,
                                     agency.email,
                                     agency.descripcion,
                                     agency.mobile_phone,
                                     agency.categories.first&.name,
                                     agency.categories.first&.categoria,
                                     agency.categories.first&.fa_name,
                                     Website.map[agency.id]["Website"],
                                     Website.map[agency.id]["Facebook"]])) -%>
<%- end -%>
