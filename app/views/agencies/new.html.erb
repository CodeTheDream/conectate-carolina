<%= form_for(@agency) do |f| %>

	<%= f.label :name %>
	<%= f.text_field :name %></br>

	<%= f.label :address %>
	<%= f.text_field :address %></br>

	<%= f.label :city %>
	<%= f.text_field :city %></br>

	<%= f.label :state %>
	<%= f.text_field :state %></br>

	<%= f.label :zipcode %>
	<%= f.text_field :zipcode %></br>

	<%= f.submit "Add a new Agency" %>

<% end %>