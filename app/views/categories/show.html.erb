<div class="center">
  <%#= will_paginate @category_articles %>
</div>
<section class="header-container">
  <%= render partial: "app/#{ENV['APP_NAME']}/header_container" %>
</section>
<div class="category_spacer">
  <div class = "panel panel-primary">
     <div class = "panel-heading">
        <h3 class = "panel-title"><%= (t'category.cat_view') %>
          <%= fa_icon @category.fa_name, text: params[:locale] == "en" ? @category.name : @category.categoria, left: true %>
        </h3>
     </div>

     <div class = "panel-body">
        <div class="list-group row">
          <%= render 'agencies/agency', obj: @category_agencies %>
        </div>
     </div>
  </div>
</div>

<div class="center">
  <%#= will_paginate @category_articles %>
</div>
